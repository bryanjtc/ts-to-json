{
  "name": "ts-to-json",
  "version": "1.7.0",
  "description": "Generate JSON schema from your Typescript sources",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": "./dist/ts-to-json",
  "author": {
    "name": "Alexander Evtushenko",
    "email": "aevtushenko@xiag.ch"
  },
  "files": [
    "dist"
  ],
  "contributors": [
    {
      "name": "Dominik Moritz",
      "email": "domoritz@gmail.com"
    },
    {
      "name": "Mo doaie",
      "email": "mdoaie@yahoo.co.uk"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cyrus-d/ts-to-json"
  },
  "license": "MIT",
  "keywords": [
    "ts",
    "typescript",
    "json",
    "schema",
    "jsonschema"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.11",
    "commander": "^9.4.1",
    "glob": "^8.0.3",
    "is-mergeable-object": "^1.1.1",
    "json5": "^2.2.2",
    "safe-stable-stringify": "^2.4.2",
    "sortobject": "^4.16.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^10.37.6",
    "@auto-it/first-time-contributor": "^10.37.6",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/animejs": "3.1.6",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "ajv": "^8.11.2",
    "ajv-formats": "^2.1.1",
    "auto": "^10.37.6",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "vega": "^5.22.1",
    "vega-lite": "^5.6.0"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsup",
    "watch": "tsc -w",
    "lint": "eslint \"{src,test,factory}/**/*.ts\"",
    "format": "yarn lint --fix",
    "test": "jest test --verbose",
    "test:fast": "cross-env FAST_TEST=1 jest test/ --verbose",
    "test:coverage": "yarn jest test/ --collectCoverage=true",
    "test:update": "cross-env UPDATE_SCHEMA=true yarn test:fast",
    "release": "yarn build && auto shipit"
  },
  "packageManager": "yarn@3.3.1"
}
